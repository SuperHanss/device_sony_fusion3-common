# mount point	fstype	device			                              options

/boot		emmc	/dev/block/platform/msm_sdcc.1/by-name/boot
/recovery	emmc	/dev/block/platform/msm_sdcc.1/by-name/FOTAKernel
/system		ext4	/dev/block/platform/msm_sdcc.1/by-name/system	      flags=backup=1;canbewiped;wipeingui
/cache		auto	/dev/block/platform/msm_sdcc.1/by-name/cache	      flags=backup=1;canbewiped;wipeingui;wipeduringfactoryreset
/data		auto	/dev/block/platform/msm_sdcc.1/by-name/userdata	      flags=backup=1;canbewiped;wipeingui;wipeduringfactoryreset
/external_sd	auto	/dev/block/mmcblk1p1 /dev/block/mmcblk1	              flags=display="External SD";storage;wipeingui;removable
/usb-otg	auto	/dev/block/sda1	/dev/block/sda		              flags=display="USB OTG";storage;wipeingui;removable
